\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthemesakura}[2016/06/09 Sakura color theme]
\definecolor{sRed}{HTML}{D5002D}
\definecolor{sDarkRed}{HTML}{B05463}
\definecolor{sDarkBlue}{HTML}{5070D7}
\definecolor{sLightGray}{gray}{0.65}
\definecolor{sDarkGray}{HTML}{312D2E}
\definecolor{sDarkPurple}{HTML}{671C7D}
\definecolor{sOKGreen}{HTML}{00CC00}
\definecolor{sXlsxGreen}{HTML}{206F42}
\definecolor{sOrange}{HTML}{F6AA00}
\definecolor{sBrown}{HTML}{663300}
\setbeamercolor{titlelike}{fg=sDarkGray}
\setbeamercolor{normal text}{fg=sDarkGray}
\setbeamercolor{palette primary}{%
  fg=sDarkGray,
  bg=white
}
\setbeamercolor{palette secondary}{%
  fg=white,
  bg=sDarkPurple
}
\setbeamercolor{frametitle}{%
  use=palette secondary,
  parent=palette secondary
}
\setbeamercolor{caption name}{%
  use=palette primary,
  parent=palette primary
}
\setbeamercolor{bibliography item}{%
  parent=palette primary
}
\setbeamercolor*{bibliography entry author}{%
  parent=palette primary
}
\setbeamercolor*{bibliography entry title}{%
  parent=palette primary
}
\setbeamercolor*{bibliography entry note}{%
  parent=palette primary
}
\setbeamercolor{itemize item}{%
  parent=palette primary
}
\mode<all>
%% End of file `beamercolorthemesakura.sty'.
